{
	"manifest_version": 2,
	"name": "Waybackify",
	"description": "Quickly open pages using Archive.org's Way Back Machine",
	"version": "0.1.6.7",
	"permissions": ["contextMenus", "tabs"],
	"icons": {
		"16": "icon-bitty.png",
		"48": "icon-medium.png",
		"128": "icon-large.png"
	},
	"background": {"scripts": ["main.js"]},
	"browser_action": {}
}